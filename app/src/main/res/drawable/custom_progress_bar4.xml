<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@android:id/progress"
        android:width="85dp"
        android:height="85dp"
        android:bottom="15dp"
        android:gravity="center"
        android:left="15dp"
        android:right="15dp"
        android:top="15dp">

        <rotate android:fromDegrees="0"
            android:toDegrees="360"
            android:pivotX="50%"
            android:pivotY="50%">
        <layer-list>

            <item
                android:width="70dp"
                android:height="70dp"
                android:gravity="center">

                <shape
                    android:gravity="center"
                    android:shape="oval">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="2dp"
                        android:color="@color/fui_bgGoogle" />

                    <solid
                        android:color="@color/colorPrimary" />
                </shape>
            </item>


            <item
                android:width="40dp"
                android:height="40dp"
                android:gravity="center">

                <shape
                    android:gravity="center"
                    android:shape="oval">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="2dp"
                        android:color="@color/fui_bgGoogle" />

                    <solid
                        android:color="@color/colorPrimary" />
                </shape>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:gravity="right|center"
                android:right="75dp"
                android:top="8dp">
                <shape
                    android:gravity="center"
                    android:shape="line">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="2dp"
                        android:color="@color/colorPrimary" />

                    <solid android:color="@color/fui_bgGoogle" />
                </shape>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:bottom="8dp"
                android:gravity="right|center"
                android:right="75dp">
                <shape
                    android:gravity="center"
                    android:shape="line">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="2dp"
                        android:color="@color/colorPrimary" />

                    <solid android:color="@color/fui_bgGoogle" />
                </shape>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:gravity="left|center"
                android:left="75dp"
                android:top="8dp">
                <shape
                    android:gravity="center"
                    android:shape="line">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="2dp"
                        android:color="@color/colorPrimary" />

                    <solid android:color="@color/fui_bgGoogle" />
                </shape>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:bottom="8dp"
                android:gravity="left|center"
                android:left="75dp">
                <shape
                    android:gravity="center"
                    android:shape="line">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="2dp"
                        android:color="@color/colorPrimary" />

                    <solid android:color="@color/fui_bgGoogle" />
                </shape>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:gravity="bottom|center"
                android:bottom="78dp"
                android:right="8dp">
                <rotate
                    android:fromDegrees="90"
                    android:toDegrees="90">
                    <shape
                        android:gravity="center"
                        android:shape="line">
                        <size
                            android:width="80dp"
                            android:height="80dp" />
                        <stroke
                            android:width="2dp"
                            android:color="@color/colorPrimary" />

                        <solid android:color="@color/fui_bgGoogle" />
                    </shape>
                </rotate>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:gravity="bottom|center"
                android:bottom="78dp"
                android:left="8dp">
                <rotate
                    android:fromDegrees="90"
                    android:toDegrees="90">
                    <shape
                        android:gravity="center"
                        android:shape="line">
                        <size
                            android:width="80dp"
                            android:height="80dp" />
                        <stroke
                            android:width="2dp"
                            android:color="@color/colorPrimary" />

                        <solid android:color="@color/fui_bgGoogle" />
                    </shape>
                </rotate>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:gravity="top|center"
                android:top="78dp"
                android:right="8dp">
                <rotate
                    android:fromDegrees="90"
                    android:toDegrees="90">
                    <shape
                        android:gravity="center"
                        android:shape="line">
                        <size
                            android:width="80dp"
                            android:height="80dp" />
                        <stroke
                            android:width="2dp"
                            android:color="@color/colorPrimary" />

                        <solid android:color="@color/fui_bgGoogle" />
                    </shape>
                </rotate>
            </item>

            <item
                android:width="15dp"
                android:height="10dp"
                android:gravity="top|center"
                android:top="78dp"
                android:left="8dp">
                <rotate
                    android:fromDegrees="90"
                    android:toDegrees="90">
                    <shape
                        android:gravity="center"
                        android:shape="line">
                        <size
                            android:width="80dp"
                            android:height="80dp" />
                        <stroke
                            android:width="2dp"
                            android:color="@color/colorPrimary" />

                        <solid android:color="@color/fui_bgGoogle" />
                    </shape>
                </rotate>
            </item>

            <item
                android:width="5dp"
                android:height="14dp"
                android:gravity="top|center"
                android:top="80dp">
                <rotate
                    android:fromDegrees="90"
                    android:toDegrees="90">
                    <shape
                        android:gravity="center"
                        android:shape="rectangle">
                        <size
                            android:width="80dp"
                            android:height="80dp" />
                        <stroke
                            android:width="1dp"
                            android:color="@color/colorPrimary" />

                        <solid android:color="@color/fui_bgGoogle" />
                    </shape>
                </rotate>
            </item>

            <item
                android:width="5dp"
                android:height="14dp"
                android:bottom="80dp"
                android:gravity="bottom|center">
                <rotate
                    android:fromDegrees="90"
                    android:toDegrees="90">
                    <shape
                        android:gravity="center"
                        android:shape="rectangle">
                        <size
                            android:width="80dp"
                            android:height="80dp" />
                        <stroke
                            android:width="1dp"
                            android:color="@color/colorPrimary" />

                        <solid android:color="@color/fui_bgGoogle" />
                    </shape>
                </rotate>
            </item>

            <item
                android:width="5dp"
                android:height="14dp"
                android:right="84dp"
                android:gravity="right|center">
                <shape
                    android:gravity="center"
                    android:shape="rectangle">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="1dp"
                        android:color="@color/colorPrimary" />

                    <solid android:color="@color/fui_bgGoogle" />
                </shape>
            </item>


            <item
                android:width="5dp"
                android:height="14dp"
                android:left="84dp"
                android:gravity="left|center">
                <shape
                    android:gravity="center"
                    android:shape="rectangle">
                    <size
                        android:width="80dp"
                        android:height="80dp" />
                    <stroke
                        android:width="1dp"
                        android:color="@color/colorPrimary" />

                    <solid android:color="@color/fui_bgGoogle" />
                </shape>
            </item>


        </layer-list>
        </rotate>
    </item>

</layer-list>